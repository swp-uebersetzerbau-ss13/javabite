DECLARE_STRUCT #3 ! c
DECLARE_DOUBLE ! ! foo
DECLARE_ARRAY #10 ! triangles
DECLARE_ARRAY #3 ! !
DECLARE_STRUCT #3 ! !
DECLARE_DOUBLE ! ! x
DECLARE_DOUBLE ! ! y
DECLARE_DOUBLE ! ! z
DECLARE_DOUBLE ! ! bar

DECLARE_STRUCT #3 ! point
DECLARE_DOUBLE ! ! x
DECLARE_DOUBLE ! ! y
DECLARE_DOUBLE ! ! z

STRUCT_SET_DOUBLE point x #0.0
STRUCT_SET_DOUBLE point y #0.0
STRUCT_SET_DOUBLE point z #0.0

DECLARE_REFERENCE ! ! t1
STRUCT_GET_REFERENCE c triangles t1

DECLARE_REFERENCE ! ! t2
ARRAY_GET_REFERENCE t1 #4 t2

DECLARE_REFERENCE ! ! t3
ARRAY_GET_REFERENCE t2 #1 t3

DECLARE_DOUBLE ! ! t4
DECLARE_DOUBLE ! ! t5
DECLARE_DOUBLE ! ! t6

STRUCT_GET_DOUBLE point x t4
STRUCT_GET_DOUBLE point y t5
STRUCT_GET_DOUBLE point z t6

STRUCT_SET_DOUBLE t3 x t4
STRUCT_SET_DOUBLE t3 y t5
STRUCT_SET_DOUBLE t3 z t6
